{
  "name": "clash-of-robots",
  "version": "0.0.0-development",
  "main": "dist/src/Game.js",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.0.4",
    "@types/mocha": "^2.2.44",
    "@types/redux": "^3.6.0",
    "@types/uuid": "^3.4.3",
    "babel-core": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "github-changes": "^1.1.1",
    "mocha": "^4.0.1",
    "nyc": "^11.3.0",
    "prettier": "^1.8.1",
    "semantic-release": "^8.2.0",
    "ts-node": "^3.3.0",
    "typescript": "^2.6.1"
  },
  "dependencies": {
    "redux": "^3.7.2",
    "uuid": "^3.1.0"
  },
  "scripts": {
    "build": "tsc",
    "test:unit": "nyc mocha -- test/**/*.spec.ts",
    "test": "npm run test:unit",
    "prepublish": "npm run build",
    "precommit": "prettier ./**/*.ts",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/morten-olsen/clash-of-robots.git"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "test/**/*"
    ],
    "reporter": [
      "lcov",
      "text-summary",
      "html"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}
